<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout5"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/atlasLayout"
        android:background="@drawable/bottom_line" >

    <TextView
        android:id="@+id/tvAtlasName"
        style="@style/tvAtlas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:text="textView5" />
        </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/atlasDescriptionLayout"
        android:background="@drawable/bottom_line_search2"
        android:layout_below="@id/atlasLayout" >

        <TextView
            android:id="@+id/tvAtlasDescription"
            style="@style/tvCalories"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:text="textView5" />
</RelativeLayout>
</RelativeLayout>